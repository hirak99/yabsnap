#compdef yabsnap

_yabsnap() {
  # Path where yabsnap.sh is also present.
  local script_dir="/usr/share/yabsnap"
  local output=("$(PYTHONPATH=$script_dir python -O -u -m code.shell_completions "${words[@]}")")
  local -a reply=("${=output}")
  reply=(${reply[@]#*( )}) # Remove any blank entries
  compadd -a reply
}

compdef _yabsnap yabsnap
